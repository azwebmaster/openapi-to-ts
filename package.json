{
  "name": "@azwebmaster/openapi-to-ts",
  "version": "0.0.8",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openapi-to-ts": "dist/cli.js",
    "ott": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "packageManager": "bun@1.2.19",
  "scripts": {
    "build": "bun tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "generate": "bun run src/index.ts",
    "cli": "bun run src/cli.ts",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "axios": "^1.7.0",
    "commander": "^12.1.0",
    "openapi-types": "^12.1.3",
    "ts-morph": "^24.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "latest",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  }
}